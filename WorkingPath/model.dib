--====== Produktions-Kommandos ================================

  command lasse_mit_Kunden_sprechen(ein_Entwickler: Entwickler) is
    "[poziadaj o] [roz]hovor so zakaznikom", ask "Kto má hovori so zákazníkom?" => ein_Entwickler;
    "poziadaj %(ein_Entwickler) o [roz]hovor so zakaznikom";
    diary_entry "%(ein_Entwickler) u vie, e sa má porozpráva so zákazníkom.";
  end command;

-----------------------------------------------------

  command lasse_spezifizieren(ein_Entwickler : Entwickler) is
    "specifikuj", ask "Kto má vykona špecifikáciu?" => ein_Entwickler;
	"poziadaj [o] specifikaciu", ask "Kto má pracova na špecifikácii?" => ein_Entwickler;
    "[poziadaj] [o] ([na]pisanie specifikacie|pracovanie na špecifikácií)", ask "Kto má pracova na špecifikácii?" => ein_Entwickler;
    "poziadaj %(ein_Entwickler) o specifikaciu";
    "poziadaj %(ein_Entwickler) o ([na]pisanie specifikacie|(pracu|pracovanie) na špecifikácií)";
    diary_entry "%(ein_Entwickler) u vie, e má pracova na špecifikácii.";
  end command;

-----------------------------------------------------

  command lasse_System_entwerfen(ein_Entwickler : Entwickler) is
    "navrhni system", ask "Kto má navrhnú systém?" => ein_Entwickler;
    "[poziadaj [o]] ([na]pisanie|vypracovanie|pracu na|pracovanie na) navrhu systemu", ask "Kto má navrhova systém?" => ein_Entwickler;
    "[poziadaj [o]] navrh systemu", ask "Kto má navrhova systém?" => ein_Entwickler;
    "poziadaj %(ein_Entwickler) o ([na]pisanie|vypracovanie|pracu na|pracovanie na) navrhu systemu";
    diary_entry "%(ein_Entwickler) u vie, e má pracova na návrhu systému.";
  end command;

-----------------------------------------------------

  command lasse_Module_spezifizieren(ein_Entwickler : Entwickler) is
    "navrhni modul", ask "Kto má pracova na návrhu modulov?" => ein_Entwickler;
	"specifikuj moduly", ask "Kto má pracova na špecifikácii modulov?" => ein_Entwickler;
    "[poziadaj [o]] (napisanie|pisanie|vypracovanie|pracovanie na|pracu na) (specifikacie|špecifikácií|navrhu) (modulu|modulov)", ask "Kto má pracova na špecifikácii modulov?" => ein_Entwickler;
    "poziadaj [o] (dizajn|specifikaciu|navrh) modulov", ask "Kto má pracova na špecifikácii modulov?" => ein_Entwickler;
    "poziadaj %(ein_Entwickler) o (napisanie|pisanie|vypracovanie|pracovanie na|pracu na) (specifikacie|špecifikácií|navrhu) (modulu|modulov)";
    "poziadaj %(ein_Entwickler) o (dizajn|specifikaciu|navrh) modulov";
    diary_entry "%(ein_Entwickler) bol(a) oboznámenı/á, e má pracova na špecifikácií modulov.";
  end command;

-----------------------------------------------------

  command lasse_codieren(ein_Entwickler : Entwickler) is
    "(napis|pis|implementuj) kód", ask "Kto má implementovat kód?" => ein_Entwickler;
    "[poziadaj] [o] (napisanie|pisanie|implementaciu|implementovanie) kódu", ask "Kto má pisat kód?" => ein_Entwickler;
    "poziadaj [o] pracu [na] implementatacii", ask "Kto má pracovat na implementacii?" => ein_Entwickler;
    "poziadaj %(ein_Entwickler) o (napisanie|pisanie|implementaciu|implementovanie) [kódu]";
    diary_entry "%(ein_Entwickler) u vie, e má pracova na implementácii.";
  end command;

