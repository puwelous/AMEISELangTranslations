--====== Produktions-Kommandos ================================

  command lasse_mit_Kunden_sprechen(ein_Entwickler: Entwickler) is
    "[ask] [to] (talk|speak) [to|with] [the] customer", ask "Kto má hovoriť so zákazníkom?" => ein_Entwickler;
    "[ask] %(ein_Entwickler) [to] (talk|speak) [to|with] [the] customer";
    diary_entry "%(ein_Entwickler) už vie, že sa má porozprávať so zákazníkom.";
  end command;

-----------------------------------------------------

  command lasse_spezifizieren(ein_Entwickler : Entwickler) is
    "[ask] [to] specify", ask "Kto má pracovať na špecifikácii?" => ein_Entwickler;
    "[ask] [to] (write|work on) [the] specification", ask "Kto má pracovať na špeifikácii?" => ein_Entwickler;
    "[ask] %(ein_Entwickler) [to] specify";
    "[ask] %(ein_Entwickler) [to] (write|work on) [the] specification";
    diary_entry "%(ein_Entwickler) už vie, že má pracovať na špecifikácii.";
  end command;

-----------------------------------------------------

  command lasse_System_entwerfen(ein_Entwickler : Entwickler) is
    "design system", ask "Kto má navrhovať systém?" => ein_Entwickler;
    "([ask] [to] (write|do|work on) [the] system design)", ask "Kto má navrhovať systém?" => ein_Entwickler;
    "[ask] [to] design [the] system", ask "Kto má navrhovať systém?" => ein_Entwickler;
    "[ask] %(ein_Entwickler) [to] (write|do|work on) [the] system design";
    "[ask] %(ein_Entwickler) [to] design [the] system";
    diary_entry "%(ein_Entwickler) už vie, že má pracovať na návrhu systému.";
  end command;

-----------------------------------------------------

  command lasse_Module_spezifizieren(ein_Entwickler : Entwickler) is
    "design modules", ask "Kto má pracovať na špecifikácii modulov?" => ein_Entwickler;
    "([ask] [to] (write|do|work on) [the] module (design|specification|spec|specs))", ask "Kto má pracovať na špecifikácii modulov?" => ein_Entwickler;
    "[ask] [to] (design|specify) [the] modules", ask "Kto má pracovať na špecifikácii modulov?" => ein_Entwickler;
    "[ask] %(ein_Entwickler) [to] (write|do|work on) [the] module (design|specification|spec|specs)";
    "[ask] %(ein_Entwickler) [to] (design|specify) [the] modules";
    diary_entry "%(ein_Entwickler) bol(a) oboznámený/á, že má pracovať na špecifikácii modulov.";
  end command;

-----------------------------------------------------

  command lasse_codieren(ein_Entwickler : Entwickler) is
    "[ask] [to] [write] [the] code", ask "Kto má písať kód?" => ein_Entwickler;
    "[ask] [to] (work|do) [on] [the] implementation", ask "Kto má pracovať na implementácii?" => ein_Entwickler;
    "[ask] %(ein_Entwickler) [to] write [the] code";
    "[ask] %(ein_Entwickler) [to] implement";
    diary_entry "%(ein_Entwickler) už vie, že má pracovať na implementácii.";
  end command;

