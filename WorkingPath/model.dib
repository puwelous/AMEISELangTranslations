  command lasse_Reviews_stattfinden
   (ein_Dokument : Dokument;
   Gutachter_1  : Entwickler;
   Gutachter_2  : Entwickler) is
    "poziadaj (dvoch|2) (vyvojarov|posudkarov) o (vykonanie|ujatie [sa]) posudku", 
      ask "Ktorı dokument má by prehodnotenı/posúdenı?" => ein_Dokument,
      ask "Kto je prvou osobou, ktorá má vykona posudok?" => Gutachter_1,
      ask "Kto je druhou osobou, ktorá má vykona posudok?" => Gutachter_2;
    "poziadaj (dvoch|2) (vyvojarov|posudkarov) o [vykonanie|ujatie [sa]] posudku dokumentu %(ein_Dokument)",
      ask "Kto je prvou osobou, ktorá má vykona posudok?" => Gutachter_1,
      ask "Kto je druhou osobou, ktorá má vykona posudok?" => Gutachter_2;
    "poziadaj %(Gutachter_1) a %(Gutachter_2) o [vykonanie|ujatie [sa]] posudku",
      ask "Ktorı dokument má by prehodnotenı/posúdenı?" => ein_Dokument;
    "poziadaj %(Gutachter_1) (a|,) %(Gutachter_2) o [vykonanie|ujatie [sa]] posudku dokumentu %(ein_Dokument)";
    diary_entry "%(Gutachter_1) a %(Gutachter_2) boli oboznámení s tım, e majú posúdi/recenzova dokument: %(ein_Dokument)";
  end command;

