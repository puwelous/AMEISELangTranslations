  command lasse_SpezReviews_mit_Kunde_stattfinden
   (Gutachter_1 : Entwickler;
   Gutachter_2 : Entwickler) is
    "poziadaj zakaznika o (ucast na (posudzovani|prehodnoteni)|(posudenie|prehodnotenie)) specifikacie",
      ask "Kto je prvou osobou, ktora ma vykonat posudok?" => Gutachter_1,
      ask "Kto je druhou osobou, ktora ma vykonat posudok?" => Gutachter_2;
    "poziadaj zakaznika o (ucast na (posudzovani|prehodnoteni)|(posudenie|prehodnotenie)) specifikacie s %(Gutachter_1)",
      ask "Kto je druhou osobou, ktora ma vykonat posudok?" => Gutachter_2;
    "poziadaj zakaznika (a|,) %(Gutachter_1) o (ucast na (posudzovani|prehodnoteni)|(posudenie|prehodnotenie)) specifikacie",
      ask "Kto je druhou osobou, ktora ma vykonat posudok?" => Gutachter_2;
    "poziadaj zakaznika (a|,) %(Gutachter_1) (a|,) %(Gutachter_2) o (ucast na (posudzovani|prehodnoteni)|(posudenie|prehodnotenie)) specifikacie";
    "poziadaj zakaznika o (ucast na (posudzovani|prehodnoteni)|(posudenie|prehodnotenie)) specifikacie [spolu] s %(Gutachter_1) (a|,) %(Gutachter_2)";
    diary_entry "%(Gutachter_1) a %(Gutachter_2) boli oboznameni s tym, ze maju posudit specifikaciu spolu so zakaznikom.";
  end command;

-----------------------------------------------------

  command lasse_HandbuchReviews_mit_Kunde_stattfinden
   (Gutachter_1 : Entwickler;
   Gutachter_2 : Entwickler) is
    "poziadaj zakaznika o (ucast na (posudzovani|prehodnoteni)|(posudenie|prehodnotenie)) ([systemovej] dokumentacie|prirucky|[pouzivatelskych] manualov)",
      ask "Kto je prvou osobou, ktora ma vykonat posudok?" => Gutachter_1,
      ask " Kto je druhou osobou, ktora ma vykonat posudok?" => Gutachter_2;
    "poziadaj zakaznika o (ucast na (posudzovani|prehodnoteni)|(posudenie|prehodnotenie)) ([systemovej] dokumentacie|prirucky|[pouzivatelskych] manualov) s %(Gutachter_1)",
      ask " Kto je druhou osobou, ktora ma vykonat posudok?" => Gutachter_2;
    "poziadaj zakaznika (a|,) %(Gutachter_1) o (ucast na (posudzovani|prehodnoteni)|(posudenie|prehodnotenie)) ([systemovej] dokumentacie|prirucky|[pouzivatelskych] manualov)",
      ask " Kto je druhou osobou, ktora ma vykonat posudok?" => Gutachter_2;
    "poziadaj zakaznika (a|,) %(Gutachter_1) (a|,) %(Gutachter_2) o (ucast na (posudzovani|prehodnoteni)|(posudenie|prehodnotenie)) ([systemovej] dokumentacie|prirucky|[pouzivatelskych] manualov)";
    "poziadaj zakaznika o (ucast na (posudzovani|prehodnoteni)|(posudenie|prehodnotenie)) ([systemovej] dokumentacie|prirucky|[pouzivatelskych] manualov) s %(Gutachter_1) (a|,) %(Gutachter_2)";
    diary_entry "%(Gutachter_1) a %(Gutachter_2) boli oboznameni s tym, ze maju posudit  pouzivatelsku prirucku spolu so zakaznikom.";
  end command;

