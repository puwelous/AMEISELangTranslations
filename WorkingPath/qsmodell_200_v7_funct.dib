  command lasse_Code_nach_Test_korrigieren
   (ein_Entwickler : Entwickler;
   ein_Bericht     : Testbericht) is
    "oprav[uj] [o]testovany kod",
      ask "Kto ma opravit kod?" => ein_Entwickler,
      ask "Ktory testovaci report ma byt pouzity?" => ein_Bericht;	  
    "((poziadaj [o] (opravu|opravovanie|korekciu|korekturu))|(korekcia|korektura)) [o]testovaneho kodu",
      ask "Kto ma opravit kod?" => ein_Entwickler,
      ask "Ktory testovaci report ma byt pouzity?" => ein_Bericht;
    "((poziadaj [o] (opravu|opravovanie|korekciu|korekturu))|(korekcia|korektura)) kodu po (teste|testoch)",
      ask "Kto ma opravit kod?" => ein_Entwickler,
      ask "Ktory testovaci report ma byt pouzity?" => ein_Bericht;
    "poziadaj %(ein_Entwickler) o (opravu|opravovanie|korekciu|korekturu) [o]testovaneho kodu",
      ask "Ktory testovaci report ma byt pouzity?" => ein_Bericht;
    "poziadaj %(ein_Entwickler) o (opravu|opravovanie|korekciu|korekturu) kodu po (teste|testoch)",
      ask "Ktory testovaci report ma byt pouzity?" => ein_Bericht;
    "((poziadaj [o] (opravu|opravovanie|korekciu|korekturu))|(korekcia|korektura)) [o]testovaneho kodu (podla|na zaklade|pomocou) [[testovacieho] reportu] %(ein_Bericht)",
      ask "Kto ma opravit kod?" => ein_Entwickler;
    "((poziadaj [o] (opravu|opravovanie|korekciu|korekturu))|(korekcia|korektura)) kodu po (teste|testoch) (podla|na zaklade|pomocou) [[testovacieho] reportu] %(ein_Bericht)",
      ask "Kto ma opravit kod?" => ein_Entwickler;
    "poziadaj %(ein_Entwickler) o (opravu|opravovanie|korekciu|korekturu) [o]testovaneho kodu (podla|na zaklade|pomocou)  [[testovacieho] reportu]  %(ein_Bericht)";
    "poziadaj %(ein_Entwickler) o (opravu|opravovanie|korekciu|korekturu) kodu po (teste|testoch)(podla|na zaklade|pomocou) [[testovacieho]reportu] %(ein_Bericht)";
    diary_entry "%(ein_Entwickler) bol(a) oboznameny(a) s tym, ze ma pracovat na oprave/korekcii vsetkych chyb v zdrojovom kode na zaklade reportu %(ein_Bericht).";
  end command;

