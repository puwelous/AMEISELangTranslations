  command lasse_Reviews_3_stattfinden
   (ein_Dokument : Dokument;
   Gutachter_1  : Entwickler;
   Gutachter_2  : Entwickler;
   Gutachter_3  : Entwickler) is
    "poziadaj (troch|3) (vyvojarov|posudkarov) o (vykonanie|ujatie [sa]) posudku", 
      ask "Ktory dokument ma byt prehodnoteny/posudeny?" => ein_Dokument,
      ask "Kto je prvou osobou, ktora ma vykonat posudok?" => Gutachter_1,
      ask "Kto je druhou osobou, ktora ma vykonat posudok?" => Gutachter_2,
      ask "Kto je tretiou osobou, ktora ma vykonat posudok?" => Gutachter_3;
    "poziadaj (troch|3) (vyvojarov|posudkarov) o posudenie [dokumentu] %(ein_Dokument)",
      ask "Kto je prvou osobou, ktora ma vykonat posudok?" => Gutachter_1,
      ask "Kto je druhou osobou, ktora ma vykonat posudok?" => Gutachter_2,
      ask "Kto je tretiou osobou, ktora ma vykonat posudok?" => Gutachter_3;
    "poziadaj %(Gutachter_1) (a|,) %(Gutachter_2) (a|,) %(Gutachter_3) o (vykonanie|ujatie [sa]) posudku",
      ask "Ktory dokument ma byt prehodnoteny/posudeny?" => ein_Dokument;
    "poziadaj %(Gutachter_1) (a|,) %(Gutachter_2) (a|,) %(Gutachter_3) o posudenie [dokumentu] %(ein_Dokument)";
    diary_entry "%(Gutachter_1), %(Gutachter_2) a %(Gutachter_3) boli oboznameni s tym, ze maju posudit/recenzovat dokument: %(ein_Dokument)";
  end command;

