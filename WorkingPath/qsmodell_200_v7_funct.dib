--====== Produktions-Kommandos ================================

  command lasse_mit_Kunden_sprechen(ein_Entwickler: Entwickler) is
    "porozpravaj sa so zakaznikom", ask "Kto ma hovorit so zakaznikom?" => ein_Entwickler;
    "poziadaj %(ein_Entwickler) o [roz]hovor so zakaznikom";
    diary_entry "%(ein_Entwickler) uz vie, ze sa ma porozpravat so zakaznikom.";
  end command;

-----------------------------------------------------

  command lasse_spezifizieren(ein_Entwickler : Entwickler) is
    "specifikuj", ask "Kto ma vykonat specifikaciu?" => ein_Entwickler;
	"poziadaj [o] specifikaciu", ask "Kto ma pracovat na specifikacii?" => ein_Entwickler;
	"%(ein_Entwickler) specifikuj";
    "[poziadaj] [o] ([na]pisanie specifikacie|pracovanie na specifikacii)", ask "Kto ma pracovat na specifikacii?" => ein_Entwickler;
    "poziadaj %(ein_Entwickler) o specifikaciu";
    diary_entry "%(ein_Entwickler) uz vie, ze ma pracovat na specifikacii.";
  end command;

-----------------------------------------------------

  command lasse_System_entwerfen(ein_Entwickler : Entwickler) is
    "navrhni system", ask "Kto ma navrhnut system?" => ein_Entwickler;
    "[poziadaj [o]] ([na]pisanie|vypracovanie|pracu na|pracovanie na) navrhu systemu", ask "Kto ma navrhovat system?" => ein_Entwickler;
    "[poziadaj [o]] navrh systemu", ask "Kto ma navrhovat system?" => ein_Entwickler;
      "%(ein_Entwickler) pracuj na navrhu systemu";
    "poziadaj %(ein_Entwickler) o ([na]pisanie|vypracovanie|pracu na|pracovanie na) navrhu systemu";
    diary_entry "%(ein_Entwickler) uz vie, ze ma pracovat na navrhu systemu.";
  end command;

-----------------------------------------------------

  command lasse_Module_spezifizieren(ein_Entwickler : Entwickler) is
    "navrhni modul", ask "Kto ma pracovat na navrhu modulov?" => ein_Entwickler;
	"specifikuj moduly", ask "Kto ma pracovat na specifikacii modulov?" => ein_Entwickler;
    "[poziadaj [o]] (napisanie|pisanie|vypracovanie|pracovanie na|pracu na) (specifikacie|specifikacii|navrhu) (modulu|modulov)", ask "Kto ma pracovat na specifikacii modulov?" => ein_Entwickler;
    "poziadaj [o] (dizajn|specifikaciu|navrh) modulov", ask "Kto ma pracovat na specifikacii modulov?" => ein_Entwickler;
       "%(ein_Entwickler) pracuj na (specifikacii|navrhu) (modulu|modulov)";
    "poziadaj %(ein_Entwickler) o (napisanie|pisanie|vypracovanie|pracovanie na|pracu na) (specifikacie|specifikacii|navrhu) (modulu|modulov)";
    "poziadaj %(ein_Entwickler) o (dizajn|specifikaciu|navrh) modulov";
    diary_entry "%(ein_Entwickler) bol(a) oboznameny/a, ze ma pracovat na specifikacii modulov.";
  end command;

-----------------------------------------------------

  command lasse_codieren(ein_Entwickler : Entwickler) is
    "(napis|pis|implementuj) kod", ask "Kto ma implementovat kod?" => ein_Entwickler;
    "[poziadaj] [o] (napisanie|pisanie|implementaciu|implementovanie) kodu", ask "Kto ma pisat kod?" => ein_Entwickler;
    "poziadaj [o] pracu [na] implementatacii", ask "Kto ma pracovat na implementacii?" => ein_Entwickler;
    "%(ein_Entwickler) (pis kod|implementuj)";
    "poziadaj %(ein_Entwickler) o (napisanie|pisanie|implementaciu|implementovanie) [kodu]";
    diary_entry "%(ein_Entwickler) uz vie, ze ma pracovat na implementacii.";
  end command;

