  message Reviewsitzung_findet_statt(Pruefling       : Dokument;
                                       Anzahl_Seiten   : integer;
                                       entdeckte_Fehler: integer;
                                       entdeckte_AFP   : real) is
    "Bolo najdenych %(entdeckte_Fehler) chyb a %(entdeckte_AFP) chybajucich funkcionalnych bodov na %(Anzahl_Seiten) dosial existujucich stranach dokumentu: %(Pruefling).";
    "Stretnutie ohladom posudku pre dokument: %(Pruefling) odhalilo %(entdeckte_Fehler) chyb a %(entdeckte_AFP) chybajucich funkcionalnych bodov na dosial preskumanych %(Anzahl_Seiten) stranach.";
    "Prehodnotenie dokumentu: %(Pruefling) ukazalo, ze na %(Anzahl_Seiten) doposial preskumanych stranach sa naslo %(entdeckte_Fehler) chyb a %(entdeckte_AFP) chybajucich funkcionalnych bodov.";
    "%(entdeckte_AFP) chybajucich funkcionalnych bodov, %(entdeckte_Fehler) chyb na len %(Anzahl_Seiten) stranach. Dokument: %(Pruefling) nebol az taky dobry, ako ste si povodne mysleli. Najdolezitejsim nateraz ostava, ze sa tie chyby nasli.";
    diary_entry "Posudok dokumentu: %(Pruefling) odhalil %(entdeckte_Fehler) chyb a %(entdeckte_AFP) chybajucich funkcionalnych bodov na doposial preskumanych %(Anzahl_Seiten) stranach.";
  end message;

  message Codereviewsitzung_findet_statt(Pruefling       : Dokument;
                                       Anzahl_Zeilen   : integer;
                                       entdeckte_Fehler: integer;
                                       entdeckte_AFP   : real) is
    "Bolo najdenych %(entdeckte_Fehler) chyb a %(entdeckte_AFP) chybajucich funkcionalnych bodov na %(Anzahl_Zeilen) riadkov zdrojoveho kodu v dokumente: %(Pruefling).";
    "Stretnutie ohladom posudku pre dokument: %(Pruefling) odhalilo %(entdeckte_Fehler) chyb a %(entdeckte_AFP) chybajucich funkcionalnych bodov na dosial preskumanych %(Anzahl_Zeilen) LOC.";
    "Prehodnotenie dokumentu: %(Pruefling)  ukazalo, ze %(entdeckte_Fehler) chyb a %(entdeckte_AFP) chybajucich funkcionalnych bodov sa naslo na %(Anzahl_Zeilen) riadkoch zdrojoveho kodu.";
    "%(entdeckte_AFP) chybajucich funkcionalnych bodov, %(entdeckte_Fehler) chyb na len %(Anzahl_Zeilen) LOC. Dokument: %(Pruefling) nebol az taky dobry, ako ste si povodne mysleli. Najdolezitejsim nateraz ostava, ze sa tie chyby nasli.";
    diary_entry "Posudok dokumentu: %(Pruefling) odhalil %(entdeckte_Fehler)  chyb a %(entdeckte_AFP) chybajucich funkcionalnych bodov na doposial preskumanych %(Anzahl_Zeilen) LOC.";
  end message;

